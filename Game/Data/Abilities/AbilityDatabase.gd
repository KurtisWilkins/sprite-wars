class_name AbilityDatabase
extends RefCounted

## Static database of all 160 abilities in Sprite Wars.
## Part 1: Fire, Water, Plant, Ice, Wind, Earth, Electric (IDs 1-77).
## Part 2 (separate file) will fill in the remaining element stubs.

static func get_all_abilities() -> Array[Dictionary]:
	var abilities: Array[Dictionary] = []
	abilities.append_array(_fire_abilities())
	abilities.append_array(_water_abilities())
	abilities.append_array(_plant_abilities())
	abilities.append_array(_ice_abilities())
	abilities.append_array(_wind_abilities())
	abilities.append_array(_earth_abilities())
	abilities.append_array(_electric_abilities())
	abilities.append_array(_dark_abilities())
	abilities.append_array(_light_abilities())
	abilities.append_array(_fairy_abilities())
	abilities.append_array(_lunar_abilities())
	abilities.append_array(_solar_abilities())
	abilities.append_array(_metal_abilities())
	abilities.append_array(_poison_abilities())
	abilities.append_array(_class_abilities())
	return abilities

static func get_ability(id: int) -> Dictionary:
	for a in get_all_abilities():
		if a.ability_id == id:
			return a
	return {}

static func get_abilities_by_element(element: String) -> Array[Dictionary]:
	var results: Array[Dictionary] = []
	for a in get_all_abilities():
		if a.element_type == element:
			results.append(a)
	return results

static func get_abilities_by_class(class_name_str: String) -> Array[Dictionary]:
	var results: Array[Dictionary] = []
	for a in get_all_abilities():
		if a.class_affinity == class_name_str:
			results.append(a)
	return results

